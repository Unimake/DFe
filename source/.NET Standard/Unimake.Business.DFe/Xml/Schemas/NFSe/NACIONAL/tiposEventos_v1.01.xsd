        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <!--TIPO COMPLEXO PARA O EVENTO DE CANCELAMENTO DE NFS-E DEFERIDO POR ANÁLISE FISCAL-->
  <xs:complexType name="TE105104">
    <xs:sequence>
      <xs:element name="xDesc">
