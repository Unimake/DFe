			</xsd:element>
			<xsd:element name="CEP" type="TSCEP" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Número do CEP</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
