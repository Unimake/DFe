		<xsd:sequence>
			<xsd:element name="Nfse" type="TCNFSe" minOccurs="1" maxOccurs="1"/>
			<xsd:element name="ListaEvento" type="tcListaEvento" minOccurs="0" maxOccurs="1"/>
