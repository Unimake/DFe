					<xsd:documentation>Grupo de informações relativas ao local da prestação do serviço</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="cServ" type="TCCServ" minOccurs="1" maxOccurs="1">
