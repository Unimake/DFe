	<!-- TIPO COMPLEXO PARA O EVENTO DE REJEIÇÃO DO INTERMEDIÁRIO -->
	<xsd:complexType name="TE204207">
		<xsd:sequence>
			<xsd:element name="xDesc" minOccurs="1" maxOccurs="1">
