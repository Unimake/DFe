			</xsd:element>
			<xsd:element name="xEstProvReg" type="TSEstadoProvRegiao" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Estado, província ou região da cidade no exterior do prestador do serviço.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
