			<xsd:pattern value="[A-Z]{2,3}[0-9]{4}|[A-Z]{3,4}[0-9]{3}"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="TSCodAcessoPed">
		<xsd:restriction base="xsd:string">
			<xsd:whiteSpace value="preserve"/>
