{
  "metadata": [
    {
      "src": [
        {
          "src": "../../",
          "files": [
            ".NET Framework/Unimake.Security.Platform/Unimake.Security.Platform.csproj",
            ".NET Standard/Unimake.Business.DFe/Unimake.Business.DFe.csproj"
          ],
          "exclude": [
            "**/bin/**",
            "**/obj/**",
            "_site/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": true,
      "disableDefaultFilter": true
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md",
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      },
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": ["default","templates/discordfx",],
    "postProcessors": ["ExtractSearchIndex"],
	"globalMetadata": {
      "_appFooter": "<span>Copyright Â© Unimake Software<br>Generated with <strong>DocFX</strong></span>",
      "_appTitle": "discordFX",
      "_enableSearch": "true",
	  "_appFaviconPath" : "myfavicon.ico"
      },
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
